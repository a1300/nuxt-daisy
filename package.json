    "happy-dom": "^13.2.0",
    "nuxt": "~3.9.3",
    "nuxt-server-fn": "^0.3.3",
    "postcss": "^8.4.33",
    "postcss-load-config": "^5.0.2",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
