{
  "name": "nuxt-daisy",
  "version": "0.1.0",
  "description": "Nuxt3 Starter with DaisyUI",
  "private": true,
  "engines": {
    "node": ">=16.15.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "format": "prettier --cache --write \"**/*.{cjs,js,jsx,ts,tsx,vue}\"",
    "lint": "eslint . --fix --ext .cjs,.js,.jsx,.ts,.tsx,.vue",
    "test": "vitest",
    "coverage": "vitest run --coverage",
