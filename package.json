{
  "name": "nuxt-daisy",
  "version": "0.1.0",
  "description": "Nuxt3 Starter with DaisyUI",
  "private": true,
  "engines": {
    "node": ">=16.15.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "format": "prettier --cache --write \"**/*.{cjs,js,jsx,ts,tsx,vue}\"",
    "lint": "eslint . --fix --ext .cjs,.js,.jsx,.ts,.tsx,.vue",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "check:all": "yarn format && yarn lint && yarn coverage"
  },
  "dependencies": {
    "pinia": "^2.1.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@nuxt/test-utils-edge": "^3.6.0-28108658.05a8c2d9",
    "@nuxtjs/google-fonts": "^3.0.1",
    "@nuxtjs/tailwindcss": "^6.7.2",
    "@pinia/nuxt": "^0.4.11",
    "@pinia/testing": "^0.1.2",
    "@tailwindcss/typography": "^0.5.9",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitest/coverage-v8": "^0.32.0",
    "@vueuse/nuxt": "^10.1.2",
    "c8": "^8.0.0",
    "daisyui": "^3.1.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.25.5",
    "eslint-plugin-vue": "^9.14.1",
    "nuxt": "~3.5.3",
    "nuxt-server-fn": "^0.3.0",
    "postcss": "^8.4.24",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "typescript": "~5.1.3",
    "vitest": "^0.32.0"
  }
}
